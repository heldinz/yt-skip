{
  "manifest_version": 2,
  "name": "Skip YT Ads",
  "version": "1.0",
  "description": "Skips and mutes YT ads",
  "content_scripts": [
    {
      "matches": ["https://www.youtube.com/*"],
      "js": ["content.js"]
    }
  ],
  "browser_specific_settings": {
    "gecko": {
      "id": "yt-skip@alicerosemurphy.com"
    }
  }
}
